{
  "name": "Empleado",
  "plural": "empleados",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellido": {
      "type": "string",
      "required": true
    },
    "nacionalidad": {
      "type": "string",
      "required": true
    },
    "edad": {
      "type": "number",
      "required": true
    },
    "anyosExperiencia": {
      "type": "number",
      "required": true
    },
    "descPerfil": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "experiencia": {
      "type": "hasMany",
      "model": "Experiencia",
      "foreignKey": "empleadoId"
    },
    "formacion": {
      "type": "hasMany",
      "model": "Formacion",
      "foreignKey": "empleadoId"
    },
    "formacionComplementaria": {
      "type": "hasMany",
      "model": "FormacionComplementaria",
      "foreignKey": "empleadoId"
    },
    "idiomas": {
      "type": "hasMany",
      "model": "IdiomaEmpleado",
      "foreignKey": "empleadoId"
    }
  },
  "acls": [],
  "methods": {}
}
